<?
foreach($this->envs as $resource_id=>$xml) {
    $resource = $this->resources[$resource_id][0];
    echo "<div class=\"resource h4 round\">".$resource->name;
    if($resource->disable == 1) {
        echo " (Removed)";
    }
    if($resource->active == 0) {
        echo " (Inactive)";
    }
    echo "</div>";


}
